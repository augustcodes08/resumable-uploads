{
  "name": "resumable-uploads",
  "version": "0.1.5",
  "description": "A JavaScript library for providing multiple simultaneous, stable, fault-tolerant and resumable/restartable uploads via the HTML5 File API that supports typescript.",
  "main": "resumable-uploads.js",
  "types": "./resumable-uploads.d.ts",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "pre-commit": "lint-staged",
    "prepare": "husky"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/augustcodes08/resumable-uploads.git"
  },
  "keywords": [
    "html5",
    "file",
    "upload"
  ],
  "author": "https://github.com/augustcodes08/resumable-uploads/graphs/contributors",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/augustcodes08/resumable-uploads/issues"
  },
  "homepage": "https://github.com/augustcodes08/resumable-uploads#readme",
  "devDependencies": {
    "@commitlint/cli": "^19.6.0",
    "@commitlint/config-conventional": "^19.6.0",
    "@eslint/js": "^9.15.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@types/jest": "^30.0.0",
    "eslint": "^9.15.0",
    "globals": "^15.12.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsdom": "^26.1.0",
    "prettier": "^3.3.3",
    "ts-jest": "^29.4.0",
    "typescript-eslint": "^8.15.0"
  },
  "lint-staged": {
    "*.{css,html,js,json,markdown,md,sass,scss,ts,tsx}": [
      "prettier --write"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
